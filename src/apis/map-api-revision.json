{
    "family": "map-api",
    "containerDefinitions": [
        {
            "name": "map-api",
            "image": "282421455628.dkr.ecr.us-east-1.amazonaws.com/map-api/map-api:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "map-api-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "map-api-8000-tcp",
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DATABASE_PORT",
                    "value": "5432"
                },
                {
                    "name": "DATABASE_HOST",
                    "value": "localhost"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/map-api",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        },
        {
            "name": "postgres",
            "image": "282421455628.dkr.ecr.us-east-1.amazonaws.com/map-api/postgres:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "postgres-5432-tcp",
                    "containerPort": 5432,
                    "hostPort": 5432,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "fanswitch"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "map-api-database"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/map-api",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::282421455628:role/ecs-role",
    "executionRoleArn": "arn:aws:iam::282421455628:role/ecs-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "7168",
    "ephemeralStorage": {
        "sizeInGiB": 25
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
