{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "map-api",
      "image": "282421455628.dkr.ecr.us-east-1.amazonaws.com/map-api:latest",
      "memory": 512,
      "essential": true,
      "portMappings": [{ "hostPort": 8000, "containerPort": 8000 }]
    },
    {
      "name": "map-admin",
      "image": "282421455628.dkr.ecr.us-east-1.amazonaws.com/map-admin:latest",
      "memory": 512,
      "essential": true,
      "portMappings": [{ "hostPort": 3001, "containerPort": 3001 }]
    },
    {
      "name": "map-web",
      "image": "282421455628.dkr.ecr.us-east-1.amazonaws.com/map-web:latest",
      "memory": 512,
      "essential": true,
      "portMappings": [{ "hostPort": 3000, "containerPort": 3000 }]
    }
  ]
}
